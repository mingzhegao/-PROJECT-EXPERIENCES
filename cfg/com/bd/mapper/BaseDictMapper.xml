<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bd.mapper.BaseDictMapper">

    <select id="selectBaseDictByTypeCode" resultType="com.bd.entity.BaseDict">
		select dict_id,dict_item_name from base_dict where dict_type_code=#{dict_type_code}
	</select>
</mapper>